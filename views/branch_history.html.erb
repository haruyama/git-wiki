<h1>Branch History</h1>

<div class="sub_nav">
  <a href="/a/branches" class="nav_link"><%= h $repo.current_branch %> branch</a><br />
  <span class="sub_nav details"><%= h @history.size %> commits</span>
</div>

<div class="content">
  <br />
  <ul>
  <% @history.each do |commit| %>
    <li>
      <% if commit != @history.first %>
        <%= h commit.sha %>
        <%= h commit.date %>
      <% else %>
        <%= h commit.date %>
      <% end %>
      &mdash;
      <%= h commit.message %>
    </li>
  <% end %>
  </ul>
</div>

